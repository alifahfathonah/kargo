		}
	})
})
loadingimage();
function loadingimage()
{
	$('img').load(function(){
		//console.log($(this).attr("id"));
		$("#"+this.id).css('background', 'none');
		//$("#"+this.id).removeClass('img-polaroid');	
	})
}

//currency convert
		function changecurrency(value)
		{		
			var expl = value.split('-');
			var id = expl[0];
			var iso_code = expl[1];
			var rate = expl[2];
			var confirmalert = expl[3];
			var cancelconfirm = expl[4];
			if (confirm(confirmalert))
			{
				loading(true);
				$.ajax ({
						url		: 'http://barcodemart.dev/web/converts_currency',
						type	: 'post',
						data	: 'id='+id+'&iso_code='+iso_code+'&rate='+rate,
						cache	: false,
						success	: function (param)
						{
							var json = eval('('+param+')');
							if (json.error == 0)
							{
								if (json.direct == 0)
								{
									window.location.replace(json.url);
								}
								else
								{
									//notif success save
									$("#notif").removeClass('alert-warning');
									$("#notif").removeClass('alert-success');
									$("#notif").addClass('alert-danger');							
									$("#notif").html(json.msg).show();
									$("#tblcart").remove();
									$("#tbshipp").remove();
									$("#addresses").remove();
									$("#tbpayment").remove();
									$("#tbsummary").remove();
								}								
							}						
							else
							{
								window.location.replace(json.url);
							}
							
							loading(false);
							success_log();
						},					
						error	: function (param)
						{
							alert('Can not convert!');
							error_log();
						}
				})
			}
			else {alert(cancelconfirm);}
		}
		
			
		
		function language(id)
		{
			loading(true);
			$.ajax ({
					url 	: 'http://barcodemart.dev/web/change_language',
					type	: 'post',
					data	: 'id='+id,
					cache	: false,
					success	: function (param)
					{						
						window.location.replace(param);
						loading(false);
						success_log();
					},
					error	: function (param)
					{
						alert(param);
						error_log();
					}
			});
			
		}

		function checkalias(value,id_customer)
		{				
			$.ajax({
				url		:'http://barcodemart.dev/web/pubstep_payment/check_alias',
				type	:'post',
				data	:'value='+value+'&id_customer='+id_customer,
				cache	:false,
				success	:function (param)
				{
					if (param == 'yes')
					{
						$('#alias1').html("<img src='http://barcodemart.dev/asset/images/icon/block.png' align='absmiddle'><font color='red'><b> Inisial alamat sudah terdaftar, silahkan masukan yang lain!</b></font><p>");
						
					}
					else if (param == 'no')
					{
						$('#alias1').html('');

					}
					success_log();
						
				},
				error : function (param)
				{
					alert(param);
					error_log();
				}
			
			})
		}		
			
		function loading(is_show)
