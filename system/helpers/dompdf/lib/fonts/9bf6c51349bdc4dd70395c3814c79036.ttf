			if (is_show == true)
			{
				$('#loading').html("<img src='http://barcodemart.dev/asset/admin/images/loader.gif'><font color='black'><br>loading...</font>");
				
			}
			else 
			{
				$("#loading").html('');
			}
		}		
		</script>
