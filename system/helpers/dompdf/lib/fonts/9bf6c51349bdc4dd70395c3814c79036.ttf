			}
			else
			{				
				//document.getElementById('panel_cc').style.display = 'none';
				$("#panel_cc").slideUp(500);
			}
		}	
	})

	//terms shipp
	$(".chkmetod").click(function(){
		if ($('[id="chkshipp"]').is(':checked'))
